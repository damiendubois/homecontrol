  "description": "",
  "main": "index.js",
  "scripts": {
    "test-client": "node ./node_modules/karma/bin/karma start --single-run",
    "test-server": "NODE_ENV=test ./node_modules/.bin/mocha --reporter nyan --recursive ./test/server/",
    "test": "npm run test-client && npm run test-server",
    "build": "gulp dist",
    "start": "gulp serve",
    "start-prod": "NODE_ENV=production pm2 start dist/server/app.js --name homecontrol-prod",
    "stop-prod": "pm2 stop homecontrol-prod -i 0",
    "start-uat": "NODE_ENV=uat  pm2 start dist/server/app.js --name homecontrol-uat",
    "stop-uat": "pm2 stop homecontrol-uat",
    "log-uat": "pm2 logs homecontrol-uat"
  },
